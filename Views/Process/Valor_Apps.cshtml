
@{
    ViewBag.Title = "Valor Menu";
}

  <div class="row ">
    <div class="col-md-4 col-sm-4 col-lg-4">
        <div class="card border-info">
            <h5 class="card-header text-white bg-danger">OEE Reporting</h5>
            <div class="card-body">
                <h5 class="card-title">Detalle de OEE Reportado</h5>
                <p class="card-text">Página que muestra el detalle del reporte OEE</p>
                <a href=@Url.Action("OeeSMT" , "LineTracking") class="btn btn-warning">Ir</a>
            </div>
        </div>
    </div>
    <div class="col-md-4 col-sm-4 col-lg-4">
        <div class="card border-info">
            <h5 class="card-header text-white bg-warning">Downtime Entry</h5>
            <div class="card-body">
                <h5 class="card-title">Detalle de Tiempo muerto</h5>
                <p class="card-text">Página que muestra el detalle del tiempo muerto en SMT</p>
                <a href=@Url.Action("DowntimeEntry" , "Process") class="btn btn-warning">Ir</a>
            </div>
        </div>
    </div>
    <div class="col-md-4 col-sm-4 col-lg-4">
        <div class="card border-info">
            <h5 class="card-header text-white bg-danger">Components Trace</h5>
            <div class="card-body">
                <h5 class="card-title">Registro de componentes en SMT</h5>
                <p class="card-text">Página que muestra los registros de componentes usados en lineas SMT</p>
                <a href=@Url.Action("ComponentTrace" , "Process") class="btn btn-warning">Ir</a>
            </div>
        </div>
    </div>
  </div>
  <br />
  <div class="row">

      <div class="col-md-4 col-sm-4 col-lg-4">
          <div class="card border-info">
              <h5 class="card-header text-white bg-danger">SMT & Services Summry</h5>
              <div class="card-body">
                  <h5 class="card-title">Rusumen de lineas y servicios en tiempo real</h5>
                  <p class="card-text">Página que muestra en tiempo real el estado de lineas SMT y servicios del sistema VALOR</p>
                  <a href=@Url.Action("Index" , "LineTracking") class="btn btn-warning">Ir</a>
              </div>
          </div>
      </div>
      <div class="col-md-4 col-sm-4 col-lg-4">
          <div class="card border-info">
              <h5 class="card-header text-white bg-danger">Registro de componentes x PCBs</h5>
              <div class="card-body">
                  <h5 class="card-title">Detalle de componentes colocados a PCBs</h5>
                  <p class="card-text">Página que muestra la relacion del Component ID vs un numero de serie de PCB y la hora a la que fue colocado</p>
                  <a href=@Url.Action("ComponentAndPCB" , "Process") class="btn btn-warning">Ir</a>
              </div>
          </div>
      </div>
      <div class="col-md-4 col-sm-4 col-lg-4">
          <div class="card border-info">
              <h5 class="card-header text-white bg-info">System Errors</h5>
              <div class="card-body">
                  <h5 class="card-title">Detalle los errores del Sistema</h5>
                  <p class="card-text">Página que muestra el hitórico de errores en el sistema</p>
                  <a href=@Url.Action("System_Erros" , "Process") class="btn btn-warning">Ir</a>
              </div>
          </div>
      </div>
  </div>
  <br />
  <div class="row">
      <div class="col-md-4 col-sm-4 col-lg-4">
