                <p class="card-text">Página que muestra la desactivación de trazabilidad</p>
                <a href=@Url.Action("Trace_Desactivation" , "Administration") class="btn btn-success">Ir</a>
            </div>
        </div>
    </div>
    <div class="col-md-4 col-sm-4 col-lg-4">
        <div class="card border-danger">
            <h5 class="card-header text-white bg-danger">Valor Declaration</h5>
            <div class="card-body">
                <h5 class="card-title">Detalle del proceso de declaración</h5>
                <p class="card-text">Página que muestra el detalle del proceso de declaración</p>
                <a href=@Url.Action("Valor_Declaration" , "Administration") class="btn btn-success">Ir</a>
            </div>
        </div>
    </div>
</div>
